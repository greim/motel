{
  "name": "motel",
  "version": "0.2.1",
  "description": "data-vacancy observer",
  "main": "lib/index.js",
  "repository": "https://github.com/greim/motel",
  "scripts": {
    "browser-test": "browserify test-app/test-app.js -o test-app/main.js && here -d test-app",
    "lint": "eslint --max-warnings 0 src/*.js",
    "test": "mocha --compilers=js:babel-register $(find src -name \"*.test.js\")",
    "compile": "babel src -d lib --ignore '**/*.test.js'",
    "build": "npm run compile && browserify lib/index.js -s motel -o lib/bundle.js",
    "sanity": "npm run lint && npm test && npm run build",
    "prepublish": "npm run sanity"
  },
  "keywords": [
    "data-fetching",
    "react",
    "redux"
  ],
  "author": "Greg Reimer <gregreimer@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-register": "^6.24.1",
    "browserify": "^14.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-babel": "^4.1.1",
    "mocha": "^3.3.0",
    "serve-here": "^3.2.0",
    "sinon": "^2.1.0"
  },
  "dependencies": {
    "url-pattern": "^1.0.3"
  }
}
